{
  "name": "pcln-popover",
  "version": "4.2.5",
  "description": "React component for popover",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "dist/"
  ],
  "author": "Priceline",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0 <9.0.0 || >=10.0.0",
    "npm": ">=5.10.0 || >=6.0.0"
  },
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel ./src --ignore '**/*.stories.js,**/*.spec.js' --out-dir ./dist/cjs",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel ./src --ignore '**/*.stories.js,**/*.spec.js' --out-dir ./dist/esm",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --verbose --coverage",
    "test:ci": "jest --silent --coverage --runInBand",
    "test:update": "jest --updateSnapshot",
    "test:ci:update": "jest --runInBand --updateSnapshot"
  },
  "dependencies": {
    "react-popper": "^1.3.3",
    "@styled-system/prop-types": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@priceline/babel-preset": "workspace:*",
    "@priceline/eslint-config": "workspace:*",
    "@reach/component-component": "^0.7.0",
    "@rushstack/eslint-patch": "^1.0.6",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
    "jest-standard-reporter": "^2.0.0",
    "jest-styled-components": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "pcln-design-system": "workspace:*",
    "pcln-icons": "workspace:*",
    "pcln-slider": "workspace:*",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-focus-lock": "^2.2.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "pcln-design-system": "^4.7.1",
    "react": ">= 16.10.0",
    "react-dom": ">= 16.10.0",
    "styled-components": ">=4.4.1"
  }
}
